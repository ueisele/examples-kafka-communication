{
  "namespace": "net.uweeisele.examples.kafka.avro.v1",
  "name": "CountEvent",
  "type": "record",
  "fields": [
    {
      "name" : "id",
      "type": {"type": "string", "logicalType": "uuid"}
    },
    {
      "name": "created",
      "type": {"type": "long", "logicalType": "timestamp-micros"}
    },
    {
      "name": "version",
      "type": "string"
    },
    {
      "name": "count",
      "type": "long"
    }
  ]
}
